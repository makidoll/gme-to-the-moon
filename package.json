{
	"name": "gme-to-the-moon",
	"version": "1.2.0",
	"scripts": {
		"start": "npm run proto && parcel watch --no-hmr src/background.ts",
		"build": "npm run proto && parcel build --no-source-maps src/background.ts",
		"proto": "pbf yaticker.proto > src/yaticker.js",
		"package": "web-ext build -i * !manifest.json !dist !icons"
	},
	"dependencies": {
		"pbf": "^3.2.1"
	},
	"devDependencies": {
		"@types/firefox-webext-browser": "^82.0.0",
		"@types/pbf": "^3.0.2",
		"parcel-bundler": "^1.12.5",
		"typescript": "^4.2.4",
		"web-ext": "^6.0.0"
	}
}
